<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.univ.mixalis.tripapp.MainActivity3">

    <TextView
        android:text="Ambulance (166)"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="46dp"
        android:id="@+id/textView2"
        tools:text="Ambulance (166)"
        android:textSize="18sp"
        android:textStyle="normal|bold"
        android:layout_below="@+id/polBtn"
        android:layout_centerHorizontal="true" />

    <TextView
        android:text="Police (100)"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView"
        tools:text="Police (100)"
        android:textSize="18sp"
        android:layout_marginTop="21dp"
        android:textStyle="normal|bold"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:textColor="@color/colorPrimaryDark" />

    <TextView
        android:text="Fire Department (199)"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="38dp"
        android:id="@+id/fireBtn"
        android:textStyle="normal|bold"
        tools:text="Fire Department (199)"
        android:textSize="18sp"
        android:textColor="@android:color/holo_red_dark"
        android:layout_below="@+id/amBtn"
        android:layout_centerHorizontal="true" />

    <Button
        android:text="Call"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/polBtn"
        tools:text="Call"
        android:layout_below="@+id/textView"
        android:layout_centerHorizontal="true" />

    <Button
        android:text="Call"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/amBtn"
        tools:text="Call"
        android:layout_below="@+id/textView2"
        android:layout_alignStart="@+id/polBtn" />

    <Button
        android:text="Call"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="11dp"
        android:id="@+id/fBtn"
        android:layout_below="@+id/fireBtn"
        android:layout_alignStart="@+id/amBtn"
        tools:text="Call" />

    <Button
        android:text="Call"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="11dp"
        android:id="@+id/firBtn"
        android:layout_below="@+id/fireBtn"
        android:layout_alignStart="@+id/amBtn"
        tools:text="Call" />

    <Button
        android:text="Call"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/euBtn"
        android:layout_alignStart="@+id/fBtn"
        android:layout_marginTop="14dp"
        android:id="@+id/euBtnn"
        tools:text="Call" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="41dp"
        android:id="@+id/euBtn"
        tools:text="European Emergency Number (112)"
        android:textSize="18sp"
        android:textStyle="normal|bold"
        android:textColor="@android:color/holo_orange_dark"
        android:layout_below="@+id/fBtn"
        android:layout_centerHorizontal="true"
        android:text="European Emergency Number (112)" />

</RelativeLayout>
